<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="300">
        <KeyFrameSet></KeyFrameSet>

    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/register_btn"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            motion:layout_constraintEnd_toStartOf="@+id/guideline5"
            motion:layout_constraintStart_toEndOf="@+id/view"
            motion:layout_constraintTop_toBottomOf="@+id/textInputLayout2">
            <CustomAttribute
                motion:attributeName="text"
                motion:customStringValue="@string/register" />
        </Constraint>
        <Constraint
            android:id="@+id/register_pb"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:elevation="10dp"
            android:visibility="gone"
            motion:layout_constraintBottom_toBottomOf="@+id/register_btn"
            motion:layout_constraintEnd_toStartOf="@+id/guideline5"
            motion:layout_constraintStart_toStartOf="@+id/register_btn"
            motion:layout_constraintTop_toTopOf="@+id/register_btn" />
        <Constraint
            android:id="@+id/textInputLayout3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/textInputLayout2"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintEnd_toStartOf="@+id/guideline5"
            motion:layout_constraintHorizontal_bias="0.5"
            motion:layout_constraintStart_toEndOf="@+id/textInputLayout2"
            motion:layout_constraintTop_toTopOf="@+id/textInputLayout2" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/register_pb"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="@+id/register_btn"
            motion:layout_constraintEnd_toStartOf="@+id/guideline5"
            motion:layout_constraintStart_toStartOf="@+id/register_btn"
            motion:layout_constraintTop_toTopOf="@+id/register_btn" />
        <Constraint
            android:id="@+id/register_btn"
            android:layout_width="@dimen/_60sdp"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            motion:layout_constraintEnd_toStartOf="@+id/guideline5"
            motion:layout_constraintTop_toBottomOf="@+id/textInputLayout2">
            <CustomAttribute
                motion:attributeName="text"
                motion:customStringValue=" " />
        </Constraint>
    </ConstraintSet>

</MotionScene>